group_by(YM) %>% filter(Date == last(Date)) %>%
ungroup() %>%
mutate(across(.cols = where(is.numeric), .fns = ~log(.) - lag(log(.)))) %>%
filter(Date > first(Date)) %>% mutate(Year = format(Date, "%Y")) %>%
